    "vue": "^2.6.11",
    "vue-draggable-resizable": "^2.3.0",
    "vue-infinite-scroll": "^2.0.2",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^3.3.11",
    "webpack-theme-color-replacer": "^1.3.26"
