    "eslint-plugin-vue": "^6.2.2",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-less": "^4.0.1",
    "gulp-strip-code": "^0.1.4",
    "husky": "^7.0.2",
